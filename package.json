{
  "name": "todo-management-webapp",
  "version": "1.0.0",
  "description": "A webapp built on express.js and vue.js for managing to-dos",
  "bin": "build/index.js",
  "main": "build/index.js",
  "files": [
    "build/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickp10/todo-management-webapp.git"
  },
  "author": "nickp10@hotmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nickp10/todo-management-webapp/issues"
  },
  "homepage": "https://github.com/nickp10/todo-management-webapp#readme",
  "scripts": {
    "build": "npm run compile && npm run copy-vue",
    "build-debug": "npm run compile-debug && npm run copy-vue-debug",
    "compile": "tsc",
    "compile-debug": "tsc --sourceMap --outDir debug/",
    "copy-assets": "npm run copy-assets-bootstrap-css && npm run copy-assets-bootstrap-js && npm run copy-assets-jquery && npm run copy-assets-popper && npm run copy-assets-vue",
    "copy-assets-bootstrap-css": "copyfiles -f node_modules/bootstrap/dist/css/*.min.css src/assets/css",
    "copy-assets-bootstrap-js": "copyfiles -f node_modules/bootstrap/dist/js/bootstrap.min.js src/assets/js",
    "copy-assets-jquery": "copyfiles -f node_modules/jquery/dist/jquery.min.js src/assets/js",
    "copy-assets-popper": "copyfiles -f node_modules/popper.js/dist/popper.min.js src/assets/js",
    "copy-assets-vue": "copyfiles -f node_modules/vue/dist/vue.min.js src/assets/js",
    "copy-vue": "copyfiles -u 1 src/**/*.vue build && copyfiles -u 1 src/assets/** build",
    "copy-vue-debug": "copyfiles -u 1 src/**/*.vue debug && copyfiles -u 1 src/assets/** debug",
    "prepare": "npm run copy-assets && npm run build"
  },
  "dependencies": {
    "argv": "0.0.2",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "express-vue": "^4.0.14",
    "jquery": "^3.3.1",
    "lowdb": "^1.0.0",
    "memorystore": "^1.6.0",
    "moment": "^2.20.1",
    "popper.js": "^1.12.9",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/argv": "0.0.4",
    "@types/body-parser": "^1.16.8",
    "@types/crypto-js": "^3.1.38",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.7",
    "@types/lowdb": "^1.0.0",
    "@types/node": "^9.4.1",
    "@types/uuid": "^3.4.3",
    "copyfiles": "^1.2.0",
    "typescript": "^2.7.1"
  }
}
