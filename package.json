{
  "name": "todo-management-webapp",
  "version": "1.3.0",
  "description": "A webapp built on express.js and vue.js for managing to-dos",
  "bin": "build/index.js",
  "main": "build/index.js",
  "files": [
    "build/",
    ".babelrc"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickp10/todo-management-webapp.git"
  },
  "author": "nickp10@hotmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nickp10/todo-management-webapp/issues"
  },
  "homepage": "https://github.com/nickp10/todo-management-webapp#readme",
  "scripts": {
    "build": "npm run compile && npm run copy-vue",
    "build-debug": "npm run compile-debug && npm run copy-vue-debug",
    "compile": "tsc",
    "compile-debug": "tsc --sourceMap --outDir debug/",
    "copy-assets": "npm run copy-assets-bootstrap-css && npm run copy-assets-bootstrap-js && npm run copy-assets-bootstrap-datetimepicker-css && npm run copy-assets-bootstrap-datetimepicker-js && npm run copy-assets-datatables-css && npm run copy-assets-datatables-js && npm run copy-assets-datatables-jquery-js && npm run copy-assets-font-awesome-css && npm run copy-assets-font-awesome-fonts && npm run copy-assets-jquery && npm run copy-assets-moment && npm run copy-assets-popper && npm run copy-assets-vue",
    "copy-assets-bootstrap-css": "copyfiles -f node_modules/bootstrap/dist/css/*.min.css src/assets/css",
    "copy-assets-bootstrap-js": "copyfiles -f node_modules/bootstrap/dist/js/bootstrap.min.js src/assets/js",
    "copy-assets-bootstrap-datetimepicker-css": "copyfiles -f node_modules/tempusdominus-bootstrap-4/build/css/tempusdominus-bootstrap-4.min.css src/assets/css",
    "copy-assets-bootstrap-datetimepicker-js": "copyfiles -f node_modules/tempusdominus-bootstrap-4/build/js/tempusdominus-bootstrap-4.min.js src/assets/js",
    "copy-assets-datatables-css": "copyfiles -f node_modules/datatables.net-bs4/css/dataTables.bootstrap4.css src/assets/css",
    "copy-assets-datatables-js": "copyfiles -f node_modules/datatables.net-bs4/js/dataTables.bootstrap4.js src/assets/js",
    "copy-assets-datatables-jquery-js": "copyfiles -f node_modules/datatables.net/js/jquery.dataTables.js src/assets/js",
    "copy-assets-font-awesome-css": "copyfiles -f node_modules/font-awesome/css/font-awesome.min.css src/assets/css",
    "copy-assets-font-awesome-fonts": "copyfiles -f node_modules/font-awesome/fonts/** src/assets/fonts",
    "copy-assets-jquery": "copyfiles -f node_modules/jquery/dist/jquery.min.js src/assets/js",
    "copy-assets-moment": "copyfiles -f node_modules/moment/min/moment.min.js src/assets/js",
    "copy-assets-popper": "copyfiles -f node_modules/popper.js/dist/umd/popper.min.js src/assets/js",
    "copy-assets-vue": "copyfiles -f node_modules/vue/dist/vue.min.js src/assets/js",
    "copy-vue": "copyfiles -u 1 src/**/*.vue build && copyfiles -u 1 src/assets/** build",
    "copy-vue-debug": "copyfiles -u 1 src/**/*.vue debug && copyfiles -u 1 src/assets/** debug",
    "prepare": "npm run copy-assets && npm run build",
    "start": "node build/index.js"
  },
  "dependencies": {
    "argv": "0.0.2",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "crypto-js": "^3.1.9-1",
    "datatables.net-bs4": "^1.10.16",
    "express": "^4.16.2",
    "express-fileupload": "^0.4.0",
    "express-session": "^1.15.6",
    "express-vue": "^4.0.14",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "lowdb": "^1.0.0",
    "memorystore": "^1.6.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.20.1",
    "popper.js": "^1.13.0",
    "tempusdominus-bootstrap-4": "^5.0.0-alpha15",
    "tempusdominus-core": "^5.0.0-alpha13",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/argv": "0.0.4",
    "@types/body-parser": "^1.16.8",
    "@types/crypto-js": "^3.1.38",
    "@types/express": "^4.11.1",
    "@types/express-fileupload": "^0.1.1",
    "@types/express-session": "^1.15.8",
    "@types/lowdb": "^1.0.0",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^9.4.6",
    "@types/uuid": "^3.4.3",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "copyfiles": "^1.2.0",
    "typescript": "^2.7.2"
  }
}
